<section class="wrapper  wrapper-section wrapper-contact-form">
    <div id="contact-form" class="container">
        {% if label_hidden %}
            {% if multiple %}
                <div{{ attributes }}>
                    {% for item in items %}
                        <div{{ item.attributes }}>{{ item.content }}
                        </div>
                    {% endfor %}
                </div>
            {% else %}
                {% for item in items %}
                    <div{{ attributes }}>{{ item.content }}
                    </div>
                {% endfor %}
            {% endif %}
        {% else %}
            <div{{ attributes }}>
                <div class="anchor-title"{{ title_attributes }}>
                    <h3 class="os-animation animated fadeIn" data-os-animation="fadeIn" data-os-animation-delay=".8s" style="animation-delay: 1s;">
                        {{ label }}
                    </h3>

                </div>
                {% if multiple %}
                <div>
                    {% endif %}
                    {% for item in items %}
                        <div class="anchor-body"{{ item.attributes }}>
                            {{ item.content }}
                        </div>
                    {% endfor %}
                    {% if multiple %}
                </div>
                {% endif %}
            </div>
        {% endif %}
    </div>
</section>